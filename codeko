local screenGui = script.Parent
local menuFrame = screenGui:WaitForChild("Frame")
local homeButton = menuFrame:WaitForChild("HomeButton")
local libraryButton = menuFrame:WaitForChild("LibraryButton")
local settingsButton = menuFrame:WaitForChild("SettingsButton")

local function onHomeButtonClicked()
    print("Home button clicked!")
  
end

local function onLibraryButtonClicked()
    print("Library button clicked!")
    
end

local function onSettingsButtonClicked()
    print("Settings button clicked!")

end

homeButton.MouseButton1Click:Connect(onHomeButtonClicked)
libraryButton.MouseButton1Click:Connect(onLibraryButtonClicked)
settingsButton.MouseButton1Click:Connect(onSettingsButtonClicked)
